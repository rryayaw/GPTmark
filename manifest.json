{
  "name": "GPTmark",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": { 
    "default_icon": "GPTmark_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/c/*",
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}